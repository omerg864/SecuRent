{
	"expo": {
		"name": "SecuRent",
		"slug": "SecuRent",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "secuRent",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/icon.png",
				"backgroundColor": "#2c14dd"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/icon.png"
		},
		"splash": {
			"backgroundColor": "#2c14dd",
			"image": "./assets/images/icon.png",
			"resizeMode": "contain"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#2c14dd"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"apiUrl": "https://securent-dev.onrender.com/api/",
			"webSocketUrl": "wss://securent-dev.onrender.com"
		}
	}
}
